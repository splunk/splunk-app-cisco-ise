<form script="prettify.js" stylesheet="application.css">
  <label>Identity Services Engine Overview</label>
  <description/>
  <fieldset autoRun="true" submitButton="false">
    <input type="time" searchWhenChanged="true">
      <default/>
    </input>
  </fieldset>
  <row>
    <single>
      <title>Total Endpoints</title>
      <searchString>sourcetype=Cisco:ISE:Syslog log_type="Passed_Authentications" OR log_type="Profile" |  `clean_mac_addr(Calling_Station_ID)` | stats dc(Calling_Station_ID)</searchString>
      <earliestTime>0</earliestTime>
      <latestTime>now</latestTime>
      <option name="underLabel">Total Number of Devices Seen (All Time)</option>
      <option name="linkView">search</option>
    </single>
    <single>
      <title>Active Endpoints</title>
      <searchString>sourcetype=Cisco:ISE:Syslog log_type="RADIUS_Accounting" | `clean_mac_addr(Calling_Station_ID)` | transaction keepevicted=true endswith=eval(Acct_Status_Type="Stop") Acct_Session_Id | search Acct_Status_Type!="Stop" | stats dc(User_Name)</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="underLabel">Number of Devices Active on Network</option>
      <option name="linkView">search</option>
    </single>
    <single>
      <title>Active Guests</title>
      <searchString>sourcetype=Cisco:ISE:Syslog log_type=Passed_Authentications IdentityGroup="*guest*" | stats distinct_count(UserName)</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="underLabel">Number of Active Guest Users</option>
      <option name="linkView">search</option>
    </single>
    <single>
      <title>Profiled Endpoints</title>
      <searchString>sourcetype=Cisco:ISE:Syslog log_type=Profiler | `clean_mac_addr(EndpointMacAddress)` | stats distinct_count(EndpointMacAddress)</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="underLabel">Number of Devices Profiled</option>
      <option name="linkView">search</option>
    </single>
  </row>
  <row>
    <chart>
      <title>Authentication Methods</title>
      <searchString>sourcetype=Cisco:ISE:Syslog log_type=Passed_Authentications | `clean_mac_addr(Calling_Station_ID)` | `clean_names(EndPointMatchedProfile)` | dedup Calling_Station_ID
| top AuthenticationMethod</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <chart>
      <title>Endpoint Distribution</title>
      <searchString>sourcetype="Cisco:ISE:Syslog" log_type="Passed_Authentications" | `clean_mac_addr(Calling_Station_ID)` | `clean_names(EndPointMatchedProfile)` | dedup Calling_Station_ID | top EndPointMatchedProfile</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>Passed Authentications</title>
      <searchString>sourcetype="Cisco:ISE:Syslog" log_type="Passed_Authentications" NOT UserName="#*" NOT UserName="" | top UserName</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisStart</option>
      <option name="charting.legend.placement">none</option>
      <option name="charting.axisTitleX.text">User Name</option>
      <option name="charting.axisTitleY.text">Number of Logins</option>
    </chart>
    <chart>
      <title>Failed Attempts</title>
      <searchString>sourcetype=Cisco:ISE:Syslog log_type="Failed_Attempts" NOT UserName="#*" NOT UserName="" | top UserName</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">none</option>
      <option name="charting.axisTitleY.text">Number of Attempts</option>
      <option name="charting.axisTitleX.text">User Name</option>
    </chart>
  </row>
  <row>
    <table>
      <title>Real-Time Authentications -4h</title>
      <searchString>sourcetype="Cisco:ISE:Syslog" log_type="Passed_Authentications" NOT UserName="#*" NOT UserName=""  | `clean_mac_addr(Calling_Station_ID)` | `clean_names(EndPointMatchedProfile)` | `clean_names(AuthorizationPolicyMatchedRule)` | table _time, UserName, Calling_Station_ID, EndPointMatchedProfile, NetworkDeviceName, NAS_Port_Id, AuthorizationPolicyMatchedRule</searchString>
      <earliestTime>-4h@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">false</option>
      <option name="table.drilldown">1</option>
      <option name="table.sortDirection">asc</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="wrap">true</option>
      <option name="displayRowNumbers">false</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">row</option>
      <option name="count">20</option>
    </table>
  </row>
</form>
